{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Sawmills",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#554bda",
    "light": "#6459f4",
    "dark": "#554bda",
    "anchors": {
      "from": "#554bda",
      "to": "#6459f4"
    }
  },
  "background": { "style": "gradient" },
  "topbarCtaButton": {
    "name": "Sign Up for Beta",
    "url": "https://www.sawmills.ai/#",
    "arrow": false
  },
  "primaryTab": {
    "name": "Guides",
    "url":"docs"
  },
  "tabs": [
    {
      "name": "Guides",
      "url": "docs"
    },
    {
      "name": "API Reference",
      "url": "api-reference",
      "openapi": "/openapi.json"
    },
    {
      "name": "Changelog",
      "url": "changelog"
    }
  ],
  "anchors": [
    {
      "name": "About",
      "icon": "house",
      "url": "https://www.sawmills.ai/"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://www.sawmills.ai/blog"
    }
  ],
  "navigation": [
    {
      "group": "home",
      "pages": ["home/home"]
    },
    {
      "group": "Changelog",
      "pages": ["changelog/changelog"]
    },
    {
      "group": "Documentation",
      "pages": [
        "docs/getting-started",
        {
          "group": "Pipelines",
          "pages": [
            "docs/pipelines",
            {
              "group": "Sources",
              "pages": ["docs/sources", "docs/source-datadog-logs"]
            },
            {
              "group": "Destinations",
              "pages": [
                "docs/destinations",
                "docs/destinations-datadog",
                "docs/destinations-aws-s3"
              ]
            },
            "docs/processors"
          ]
        },
        {
          "group": "Quota Management",
          "pages": [
            "docs/quota-management",
            "docs/create-new-quota",
            "docs/edit-quota",
            "docs/delete-quota",
            "docs/manage-quota"
          ]
        }
      ]
    }
  ]
}
